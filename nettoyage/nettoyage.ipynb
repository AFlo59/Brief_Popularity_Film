    "from db.database_mysql import engine\n",
    "\n",
    "df = pd.read_sql_query('''SELECT jp.title, jp.`year`, jp.director, jp.country, jp.duration, jp.genre, jp.first_day, jp.first_week, jp.first_weekend, jp.hebdo_rank, jp.total_spectator, jp.copies, fa1.distributor,fa1.rating_press, fa1.rating_public, fa1.casting, fa1.budget, fa1.lang, fa1.visa, fa1.award \n",
    "FROM films_jp as jp\n",
    "LEFT JOIN films_allo fa1 ON fa1.id_jp = jp.id                     \n",
    "  where fa1.year_allo is not null and fa1.year_allo != -1\n",
    "  group by fa1.id_jp, jp.`year`, jp.director''', engine)\n",
    "\n",
    "df.head()"
   ]
